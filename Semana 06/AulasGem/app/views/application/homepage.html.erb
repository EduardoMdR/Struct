<h1>Homepage</h1>

<% if current_user %>
  <h2> Email : <%= current_user.email %> está logado </h2>
  <% if current_user.photo.present? %>
    <%= image_tag current_user.photo, style: 'width:100px' %>
  <% else %>
    <h4> Usuário sem foto </h2>
  <% end %>
<% else %>
  <h2> Usuario não logado </h2>
<% end %>